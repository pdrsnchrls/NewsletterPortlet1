<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 7.2.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_7_2_0.dtd">

<routes>
	<route>
		<pattern>/{issueNumber:\d+}/{issueId:\d+}</pattern>
		<implicit-parameter name="p_p_id">com_liferay_amf_newsletter_web_portlet_NewsletterPortlet</implicit-parameter>
		<implicit-parameter name="mvcRenderCommandName">/issue/view</implicit-parameter>
		<implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
		<implicit-parameter name="p_p_mode">view</implicit-parameter>
	</route>
	<route>
		<pattern>/{newsletterId:\d+}/issue/{issueId:\d+}</pattern>
		<implicit-parameter name="mvcRenderCommandName">/article/view</implicit-parameter>
		<implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
		<implicit-parameter name="p_p_id">com_liferay_amf_newsletter_web_portlet_NewsletterPortlet</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
		<implicit-parameter name="p_p_mode">view</implicit-parameter>
	</route>
	<route>
		<pattern>/search</pattern>
		<implicit-parameter name="mvcRenderCommandName">/search/view</implicit-parameter>
		<implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
		<implicit-parameter name="p_p_id">com_liferay_amf_newsletter_web_portlet_NewsletterPortlet</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
		<implicit-parameter name="p_p_mode">view</implicit-parameter>
	</route>
</routes>